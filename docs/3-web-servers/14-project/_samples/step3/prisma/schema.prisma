generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id        Int       @id @default(autoincrement())
  title     String
  due_at    DateTime?
  createdAt DateTime  @default(now())
}
